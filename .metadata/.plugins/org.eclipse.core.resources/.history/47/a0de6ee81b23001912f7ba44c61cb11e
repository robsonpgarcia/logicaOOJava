	// paras lista,
	// senao expoe para fora do programa a lista
	// sera criado metodos para fazer esse trabalho

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}
	
	public Zoologico() {
		
	}

}
